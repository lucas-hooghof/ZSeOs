ENTRY(_start)
OUTPUT_FORMAT(binary)

SECTIONS
{
    . = 0x8000;

    .text : {
        *(.text)
        *(.text.*)
    }

    .rodata : 
    {
        *(.rodata)
        *(.rodata.*)
    }

    .data :
    {
        *(.data)
        *(.data.*)
    }

    .bss :
    {
        *(.bss)
        *(.data.*)
    }
}